<Window x:Class="File_Processor.Views.MaliciousWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:File_Processor.Views"
        mc:Ignorable="d"
        Title="Malicious Window" Height="150" Width="300">
    <Grid>
        <Label Name="MaliciousLable" Content="" HorizontalAlignment="Center" VerticalAlignment="Top" Width="254" Height="42" Margin="0,5,0,0"/>
        <Button Name="DeleteFile" Content="Delete File" Click="DeleteFile_Click" HorizontalAlignment="Left" Height="20" Width="70" Margin="37,52,0,12"/>
        <Button Name="ProcessFile" Content="Process File" Click="ProcessFile_Click" HorizontalAlignment="Center" Height="20" Width="70" Margin="0,52,0,12"/>
        <Button Name="IgnoreFile" Content="Ignore File" Click="IgnoreFile_Click" HorizontalAlignment="Right" Height="20" Width="70" Margin="0,52,37,12"/>
    </Grid>
</Window>
